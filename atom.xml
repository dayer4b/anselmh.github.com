---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
	<feed xmlns="http://www.w3.org/2005/Atom">
		<title>Anselm Hannemann</title>
		<link href="http://helloanselm.com/atom.xml" rel="self"/>
		<link href="http://helloanselm.com/"/>
		<updated>{{ site.time | date_to_xmlschema }}</updated>
		<id>http://helloanselm.com/</id>
		<author>
			<name>Anselm Hannemann</name>
			<email>hello@anselm-hannemann.com</email>
		</author>
		{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://helloanselm.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://helloanselm.com.com{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
		{% endfor %}
	</feed>